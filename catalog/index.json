{
  "version": "1.0",
  "apiVersion": "catalog.v1",
  "kind": "ConnectorCatalog",
  "metadata": {
    "name": "noesis-connectors",
    "description": "Official Noesis Connector Catalog",
    "version": "1.0.0",
    "publisher": "Data Power",
    "labels": {
      "organization": "data-power",
      "tier": "official",
      "support": "community"
    },
    "created": "2025-01-22T10:00:00Z",
    "updated": "2025-12-10T14:22:00Z"
  },
  "connectors": {
    "postgres": {
      "name": "postgres",
      "displayName": "PostgreSQL Source Connector",
      "description": "Extract data from PostgreSQL databases with CDC support",
      "image": "docker.io/benoittailhades/noesis-postgres-connector@sha256:5238d08c57a9c85136b87bbe7634df474e5a41589a3b471825534000aabfe91e",
      "semver": "1.0.0",
      "api": "connector.v1",
      "protocol": "grpc",
      "kind": "source",
      "capabilities": {
        "discovery": true,
        "checkConnection": true,
        "incremental": true,
        "cdc": false,
        "schema_evolution": true,
        "partitioning": false,
        "filtering": true,
        "auth": [
          "password",
          "ssl"
        ],
        "encryption": [
          "tls",
          "ssl"
        ]
      },
      "configSchema": {
        "type": "object",
        "required": [
          "hostname",
          "port",
          "database",
          "username",
          "password"
        ],
        "properties": {
          "hostname": {
            "type": "string",
            "title": "Database Host",
            "description": "PostgreSQL server hostname or IP address",
            "pattern": "^[a-zA-Z0-9.-]+$",
            "examples": [
              "localhost",
              "postgres.example.com",
              "192.168.1.100"
            ]
          },
          "port": {
            "type": "integer",
            "title": "Port",
            "description": "PostgreSQL server port number",
            "default": 5432,
            "minimum": 1,
            "maximum": 65535
          },
          "database": {
            "type": "string",
            "title": "Database Name",
            "description": "Name of the PostgreSQL database to connect to",
            "minLength": 1,
            "maxLength": 64,
            "pattern": "^[a-zA-Z_][a-zA-Z0-9_]*$"
          },
          "username": {
            "type": "string",
            "title": "Username",
            "description": "Database username for authentication",
            "minLength": 1,
            "maxLength": 64
          },
          "password": {
            "type": "string",
            "title": "Password",
            "description": "Database password for authentication",
            "format": "password",
            "secret": true,
            "minLength": 1
          },
          "sslMode": {
            "type": "string",
            "title": "SSL Mode",
            "description": "SSL connection mode for secure connections",
            "enum": [
              "disable",
              "require",
              "verify-ca",
              "verify-full",
              "prefer"
            ],
            "default": "prefer"
          },
          "schema": {
            "type": "string",
            "title": "Schema",
            "description": "Database schema to extract data from",
            "default": "public",
            "pattern": "^[a-zA-Z_][a-zA-Z0-9_]*$"
          },
          "connectTimeout": {
            "type": "integer",
            "title": "Connection Timeout",
            "description": "Connection timeout in seconds",
            "default": 30,
            "minimum": 1,
            "maximum": 300
          },
          "statementTimeout": {
            "type": "integer",
            "title": "Statement Timeout",
            "description": "Statement execution timeout in seconds",
            "default": 300,
            "minimum": 1,
            "maximum": 3600
          }
        }
      },
      "labels": {
        "org.yourtool.api": "connector.v1",
        "org.yourtool.protocol": "grpc",
        "org.yourtool.vendor": "data-power",
        "org.yourtool.category": "database",
        "org.yourtool.tier": "stable"
      },
      "platforms": [
        "linux/amd64"
      ],
      "docsUrl": "https://docs.noesis.dev/connectors/postgres",
      "minRunner": "1.0.0",
      "resources": {
        "requests": {
          "cpu": "100m",
          "memory": "256Mi"
        },
        "limits": {
          "cpu": "500m",
          "memory": "1Gi"
        }
      }
    },
    "tdp": {
      "name": "tdp",
      "displayName": "TDP (Technical Data Package) Source Connector",
      "description": "Extract metadata from Technical Data Packages (TDP) stored in S3/MinIO, including BOM manifests, CAD model metadata, and document metadata",
      "image": "docker.io/benoittailhades/noesis-tdp-connector@sha256:87eb6e196a2182b84231d10a708a35a79a819c5f4b6fee989f6f494830f82df8",
      "semver": "1.0.0",
      "api": "connector.v1",
      "protocol": "grpc",
      "kind": "source",
      "capabilities": {
        "discovery": true,
        "checkConnection": true,
        "incremental": false,
        "cdc": false,
        "schema_evolution": false,
        "partitioning": false,
        "filtering": false,
        "auth": [
          "access_key"
        ],
        "encryption": [
          "tls"
        ]
      },
      "configSchema": {
        "type": "object",
        "required": [
          "endpoint",
          "accessKeyId",
          "secretAccessKey",
          "bucket",
          "tdpPath"
        ],
        "properties": {
          "endpoint": {
            "type": "string",
            "title": "S3/MinIO Endpoint",
            "description": "S3 or MinIO server endpoint URL",
            "examples": [
              "minio.example.com",
              "s3.amazonaws.com",
              "localhost:9000"
            ]
          },
          "accessKeyId": {
            "type": "string",
            "title": "Access Key ID",
            "description": "S3/MinIO access key ID for authentication",
            "minLength": 1
          },
          "secretAccessKey": {
            "type": "string",
            "title": "Secret Access Key",
            "description": "S3/MinIO secret access key for authentication",
            "format": "password",
            "secret": true,
            "minLength": 1
          },
          "bucket": {
            "type": "string",
            "title": "Bucket Name",
            "description": "S3 bucket containing the TDP package",
            "minLength": 1,
            "pattern": "^[a-z0-9][a-z0-9.-]*[a-z0-9]$"
          },
          "tdpPath": {
            "type": "string",
            "title": "TDP Path",
            "description": "Path to the TDP folder within the bucket",
            "examples": [
              "TDP_Landing_Gear_Full_Package",
              "packages/tdp-001"
            ]
          },
          "useSsl": {
            "type": "boolean",
            "title": "Use SSL",
            "description": "Use HTTPS for S3/MinIO connections",
            "default": true
          },
          "region": {
            "type": "string",
            "title": "AWS Region",
            "description": "AWS region for S3 (ignored for MinIO)",
            "default": "us-east-1",
            "examples": [
              "us-east-1",
              "eu-west-1",
              "ap-southeast-1"
            ]
          }
        }
      },
      "labels": {
        "org.yourtool.api": "connector.v1",
        "org.yourtool.protocol": "grpc",
        "org.yourtool.vendor": "data-power",
        "org.yourtool.category": "file-system",
        "org.yourtool.tier": "stable"
      },
      "platforms": [
        "linux/amd64"
      ],
      "docsUrl": "https://docs.noesis.dev/connectors/tdp",
      "minRunner": "1.0.0",
      "resources": {
        "requests": {
          "cpu": "100m",
          "memory": "128Mi"
        },
        "limits": {
          "cpu": "500m",
          "memory": "512Mi"
        }
      }
    }
  },
  "annotations": {
    "catalog.noesis.io/last-refresh": "2025-01-22T10:00:00Z",
    "catalog.noesis.io/connector-count": "2",
    "catalog.noesis.io/license": "Apache-2.0"
  }
}
